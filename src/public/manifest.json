{
  "manifest_version": 2,
  "name": "hee",
  "description": "A playlist extractor for naver vibe",
  "version": "0.1.0",
  "icons": {
    "32": "icons/beasts-30.png"
  },
  "permissions": [
    "*://apis.naver.com/vibeWeb/musicapiweb/myMusic/*",
    "activeTab",
    "cookies"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icons/beasts-30.png",
    "default_title": "browser-extension",
    "default_popup": "popup.html"
  }
}